<div class="container text-center">
    <div>
        <label class="mt-2">Pin Name: </label>
        <h2 class="" id="randomName"><%= randomPin.pinName %></h2>
    </div>
    <div>
        <label class="mt-2">Category: </label>                                     
        <p id="randomCat"><%= randomPin.pinCategory %></p>
    </div>
    <div>
        <label class="mt-2">Comments: </label>                                     
        <h5 id="randomComments"><%= randomPin.comments %></h5>
    </div>
    <div>
        <label class="mt-2">Author: </label>          
        <h5 id="randomUsername"><%= randomPin.username %></h5>
    </div>
    <div id="randomId" title="<%= randomPin._id %>"></div>
</div>